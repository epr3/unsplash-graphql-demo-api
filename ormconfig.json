{
  "type": "sqlite",
  "database": "db_dev.sqlite",
  "synchronize": false,
  "logging": false,
  "entities": [
    "src/models/*.ts"
  ],
  "migrations": [
    "src/migrations/*.ts"
  ],
  "subscribers": [
    "src/subscribers/*.ts"
  ],
  "cli": {
    "entitiesDir": "src/models",
    "migrationsDir": "src/migrations",
    "subscribersDir": "src/subscribers"
  }
}